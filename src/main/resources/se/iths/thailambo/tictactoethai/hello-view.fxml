<AnchorPane stylesheets="@css/styles.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="se.iths.thailambo.tictactoethai.TicTacToeController">
    <children>
        <GridPane alignment="CENTER" gridLinesVisible="true" AnchorPane.bottomAnchor="100.0"
                  AnchorPane.leftAnchor="100.0" AnchorPane.rightAnchor="100.0" AnchorPane.topAnchor="100.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label onMouseClicked="#labelClicked" fx:id="label1" maxHeight="1.7976931348623157E308"
                       maxWidth="1.7976931348623157E308" text="O"/>
                <Label onMouseClicked="#labelClicked" fx:id="label2" maxHeight="1.7976931348623157E308"
                       maxWidth="1.7976931348623157E308" text="X"
                       GridPane.columnIndex="1"/>
                <Label onMouseClicked="#labelClicked" fx:id="label3" maxHeight="1.7976931348623157E308"
                       maxWidth="1.7976931348623157E308"
                       GridPane.columnIndex="2"/>
                <Label onMouseClicked="#labelClicked" fx:id="label4" maxHeight="1.7976931348623157E308"
                       maxWidth="1.7976931348623157E308"
                       GridPane.rowIndex="1"/>
                <Label onMouseClicked="#labelClicked" fx:id="label5" maxHeight="1.7976931348623157E308"
                       maxWidth="1.7976931348623157E308"
                       GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                <Label onMouseClicked="#labelClicked" fx:id="label6" maxHeight="1.7976931348623157E308"
                       maxWidth="1.7976931348623157E308"
                       GridPane.columnIndex="2" GridPane.rowIndex="1"/>
                <Label onMouseClicked="#labelClicked" fx:id="label7" maxHeight="1.7976931348623157E308"
                       maxWidth="1.7976931348623157E308"
                       GridPane.rowIndex="2"/>
                <Label onMouseClicked="#labelClicked" fx:id="label8" maxHeight="1.7976931348623157E308"
                       maxWidth="1.7976931348623157E308"
                       GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                <Label onMouseClicked="#labelClicked" fx:id="label9" maxHeight="1.7976931348623157E308"
                       maxWidth="1.7976931348623157E308"
                       GridPane.columnIndex="2" GridPane.rowIndex="2"/>
            </children>
            <opaqueInsets>
                <Insets bottom="100.0" left="100.0" right="100.0" top="100.0"/>
            </opaqueInsets>
        </GridPane>
        <Label fx:id="playerScore" layoutX="100.0" layoutY="246.0" text="Label" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="100.0" />
        <Label fx:id="computerScore" layoutX="316.0" layoutY="246.0" text="Label" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="100.0" />
    </children>
</AnchorPane>

